    "num=int(input(\"Enter the number:\"))\n",
    "temp=num\n",
    "sum=0\n",
    "while temp>0:\n",
    "    digit=temp%10\n",
    "    sum+=digit\n",
    "    temp//=10\n",
    "print(\"Latest value of sum:\",sum)\n",
    "print(\"Latest value of temp:\",temp)\n",
    "if num%sum==0:\n",
    "    print(num,\"is nivens number\")\n",
    "else:\n",
    "    print(num,\"is not nivens number\")"
   ]
